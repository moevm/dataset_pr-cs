all: menu.o min.o max.o diff.o sum.o
	gcc -std=gnu99 menu.o min.o max.o diff.o sum.o -o menu

menu.o: menu.c
	gcc -std=gnu99 -c menu.c

min.o: min.c
	gcc -std=gnu99 -c min.c 
    
max.o: max.c
	gcc -std=gnu99 -c max.c
    
diff.o: diff.c
	gcc -std=gnu99 -c diff.c
    
sum.o: sum.c
	gcc -std=gnu99 -c sum.c